{
  "name": "evm2wasm",
  "version": "0.0.0",
  "description": "This is a JS protope of a EVM to eWASM transcompiler",
  "main": "index.js",
  "scripts": {
    "test": "node --expose-wasm ./tests/allRunner.js",
    "vmTest": "node --expose-wasm ./tests/runVmTests.js | tap-bail",
    "build": "node ./wasm/generateInterface.js && node ./wasm/generateJson.js",
    "lint": "standard"
  },
  "author": "mjbecze <mjbecze@gmail.com>",
  "license": "MPL-2.0",
  "devDependencies": {
    "async": "^2.0.0-rc.6",
    "bn.js": "^4.11.6",
    "ethereumjs-testing": "0.0.1",
    "ethereumjs-vm": "^1.4.0",
    "ewasm-kernel": "git+https://github.com/ewasm/ewasm-kernel.git",
    "minimist": "^1.2.0",
    "standard": "^7.1.2",
    "tap-bail": "0.0.0",
    "tape": "^4.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ewasm/evm2wasm.git"
  },
  "bugs": {
    "url": "https://github.com/ewasm/evm2wasm/issues"
  },
  "homepage": "https://github.com/ewasm/evm2wasm",
  "keywords": [
    "ethereum",
    "webassembly",
    "wasm",
    "ewasm",
    "transcompiler"
  ],
  "dependencies": {
    "bulk-require": "^1.0.0",
    "bulkify": "^1.4.2",
    "ethereumjs-util": "^4.5.0"
  },
  "standard": {
    "ignore": [
      "/tools/",
      "/wasm/"
    ]
  }
